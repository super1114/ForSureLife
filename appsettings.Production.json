{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=tcp:kalilifedb.database.windows.net,1433;Initial Catalog=#{DB_NAME}#;Persist Security Info=False;User ID=#{DB_USER}#;Password=#{DB_PASSWORD}#;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "BlobSettings": {
    "ContainerName": "#{BLOB_NAME}#",
    "ConnectionString": "#{BLOB_CONNECTION_STRING}#"
  },
  "FTPSettings": {
    "Folder": "#{FTP_FOLDER_NAME}#",
    "Host": "#{FTP_HOST}#",
    "User": "#{FTP_USER}#",
    "Password": "#{FTP_PASSWORD}#"
  }
}
